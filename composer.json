{
    "name": "example/vulnerable-app",
    "description": "An example PHP application with outdated dependencies.",
    "require": {
        "symfony/symfony": "2.7.*",             
        "guzzlehttp/guzzle": "6.3.0",
        "doctrine/orm": "2.4.8",                
        "twig/twig": "1.42.0",                   
        "monolog/monolog": "1.24.0"              
    },
    "require-dev": {
        "phpunit/phpunit": "4.8.*"               
    },
    "config": {
        "optimize-autoloader": true,
        "platform": {
            "php": "7.1.0"                       
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    }
}
